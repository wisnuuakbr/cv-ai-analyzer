	"type": "commonjs",
	"main": "src/index.js",
	"scripts": {
		"dev": "nodemon app.js",
		"db:create-migration": "npx sequelize-cli migration:generate --name",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:create-seeder": "npx sequelize-cli seed:generate --name",
		"db:seed": "npx sequelize-cli db:seed:all",
		"ingest": "node scripts/ingestDocuments.js",
		"test": "jest --coverage",
		"test:watch": "jest --watch",
		"test:unit": "jest --testPathPattern=__tests__ --coverage"
	},
	"dependencies": {
		"@huggingface/inference": "^2.6.4",
