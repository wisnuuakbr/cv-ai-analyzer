		"dotenv": "^16.3.1",
		"express": "^5.1.0",
		"express-rate-limit": "^7.1.5",
		"helmet": "^7.1.0",
		"ioredis": "^5.3.2",
		"joi": "^18.0.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.11.3",
		"pdf-parse": "^1.1.1",
		"sequelize": "^6.35.2",
		"winston": "^3.12.0"
	},
	"devDependencies": {
		"jest": "^29.7.0",
